# Optimized Precog Miner Configuration
# Generated for better performance

# Network Configuration
NETWORK=finney
NETUID=55
SUBTENSOR_ENDPOINT=wss://entrypoint-finney.opentensor.ai:443

# Miner Configuration
FORWARD_FUNCTION=advanced_ensemble_miner
LOGGING_LEVEL=debug
PREDICTION_INTERVAL=5
MAX_RETRIES=5
TIMEOUT=45
CACHE_SIZE=2000

# Performance Monitoring Settings
PERFORMANCE_MONITORING=true
PERFORMANCE_LOG_INTERVAL=300
ACCURACY_TRACKING=true
INTERVAL_OPTIMIZATION=true

# Advanced Ensemble Settings
META_LEARNING=true
ADAPTIVE_WEIGHTS=true
MARKET_REGIME_DETECTION=true
PRICE_VALIDATION=true
VALIDATION_THRESHOLD=0.15

# Optimization Settings
PREDICTION_ACCURACY_TARGET=0.85
INTERVAL_SCORE_TARGET=0.80
PERFORMANCE_IMPROVEMENT_TRACKING=true

# Ensemble Weight Configuration (optimized for better performance)
ENSEMBLE_WEIGHT_BASE=0.35
ENSEMBLE_WEIGHT_ML=0.35
ENSEMBLE_WEIGHT_TECHNICAL=0.30

# Rate Limiting Settings
API_RATE_LIMIT_DELAY=2
CACHE_TIMEOUT=30
MAX_API_FAILURES=3

# Performance Optimization
ENABLE_PERFORMANCE_TRACKING=true
PERFORMANCE_HISTORY_SIZE=100
ACCURACY_THRESHOLD=0.05
INTERVAL_SCORE_THRESHOLD=0.80

# Debugging and Monitoring
ENABLE_DETAILED_LOGGING=true
LOG_PERFORMANCE_METRICS=true
TRACK_PREDICTION_ACCURACY=true
MONITOR_API_FAILURES=true
